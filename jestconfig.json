{
	"transform": {
		"^.+\\.(t|j)sx?$": "ts-jest"
	},
	"testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
	"moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
	"collectCoverage": true,
	"collectCoverageFrom": [
		"src/**/*.ts"
	],
	"verbose": true,
	"roots": [
		"./test",
		"./src"
	]
}